                        <DataTemplate>
                            <Grid ScrollViewer.VerticalScrollBarVisibility="Auto">
                                <Grid.Style>
                                    <Style TargetType="{x:Type Grid}">
                                        <Style.Triggers>
                                            <MultiDataTrigger>
                                                <MultiDataTrigger.Conditions>
                                                    <Condition Binding="{Binding Service}" Value="1" />
                                                </MultiDataTrigger.Conditions>
                                                <Setter Property="Background" Value="#33008000" />
                                            </MultiDataTrigger>
                                            <MultiDataTrigger>
                                                <MultiDataTrigger.Conditions>
                                                    <Condition Binding="{Binding Service}" Value="0" />
                                                </MultiDataTrigger.Conditions>
                                                <Setter Property="Background" Value="#33FF0000" />
                                            </MultiDataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Grid.Style>
                                <Grid.ContextMenu>
                                    <ContextMenu>
                                        <MenuItem Header="Add" Click="SkillList_Add"></MenuItem>
                                        <MenuItem Header="Remove" Click="SkillList_Remove"></MenuItem>
                                    </ContextMenu>
                                </Grid.ContextMenu>
                                <Grid>
                                    <TextBlock Text="{Binding Basic_Code}" Margin="0,5,0,0" TextWrapping="Wrap" />
                                    <Grid VerticalAlignment="Bottom" Margin="0,30,0,5">
                                        <TextBlock TextWrapping="Wrap" Margin="0,0,33,0" Text="{Binding Media_SkillName}" HorizontalAlignment="Left">
                                            <TextBlock.Style>
                                                <Style TargetType="{x:Type TextBlock}">
                                                    <Style.Triggers>
                                                        <MultiDataTrigger>
                                                            <MultiDataTrigger.Conditions>
                                                                <Condition Binding="{Binding Media_SkillToolTip}" Value="xxx" />
                                                            </MultiDataTrigger.Conditions>
                                                            <Setter Property="Visibility" Value="Hidden" />
                                                        </MultiDataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </TextBlock.Style>
                                        </TextBlock>
                                        <TextBlock Text="{Binding ID}" HorizontalAlignment="Right" />
                                    </Grid>
                                </Grid>
                            </Grid>
                        </DataTemplate>
                    </ListView.ItemTemplate>
                </ListView>

                <Grid Margin="0,0,0,5" Height="35" VerticalAlignment="Bottom">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="9*"/>
                        <ColumnDefinition Width="73*"/>
                    </Grid.ColumnDefinitions>
                    <TextBox Height="25" x:Name="RefSkill_Search" TextWrapping="WrapWithOverflow" TextChanged="RefSkill_Search_TextChanged" Grid.ColumnSpan="2" Margin="0,5" />
                </Grid>
            </Grid>
