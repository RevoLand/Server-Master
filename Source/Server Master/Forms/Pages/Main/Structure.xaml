                    <UniformGrid Rows="2">
                        <Button x:Name="refObjCommon_Structure_ObjStructure_Save" Content="Save" VerticalAlignment="Top" HorizontalAlignment="Center" MinHeight="25" MinWidth="256" Click="RefObjCommon_Structure_ObjStructure_Save_Click" />
